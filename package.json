{
  "name": "dice-utils",
  "version": "1.1.1",
  "description": "A set of utilities to parse dice notation strings and roll dice.",
  "main": "dist/dice-utils.umd.js",
  "module": "dist/dice-utils.js",
  "scripts": {
    "test": "jest /",
    "coverage": "jest / --collect-coverage",
    "build": "rollup --config"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aLittleTooQuiet/dice-utils.git"
  },
  "keywords": [
    "Dice"
  ],
  "author": "Graham De Young",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aLittleTooQuiet/dice-utils/issues"
  },
  "homepage": "https://github.com/aLittleTooQuiet/dice-utils#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "babel-jest": "^25.1.0",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.7.0",
    "jest": "^25.1.0",
    "jest-cli": "^25.1.0",
    "rollup": "^1.31.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
