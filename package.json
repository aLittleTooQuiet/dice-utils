{
  "name": "dice-utils",
  "version": "2.0.1",
  "type": "module",
  "description": "A set of utilities to parse dice notation strings and roll dice.",
  "main": "dist/dice-utils.umd.js",
  "module": "dist/dice-utils.js",
  "scripts": {
    "test": "jest /",
    "coverage": "jest / --collect-coverage",
    "lint": "eslint ./src",
    "build": "rollup --config"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aLittleTooQuiet/dice-utils.git"
  },
  "keywords": [
    "Dice"
  ],
  "author": "Graham De Young",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aLittleTooQuiet/dice-utils/issues"
  },
  "homepage": "https://github.com/aLittleTooQuiet/dice-utils#readme",
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.2",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "cross-env": "7.0.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jest": "23.18.0",
    "jest": "^29.6.1",
    "jest-cli": "^29.6.1",
    "rollup": "^3.26.3",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "ts-jest"
    },
    "coverageThreshold": {
      "global": {
